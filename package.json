{
  "name": "devreplay",
  "version": "0.3.0",
  "description": "Replay your source code development",
  "bin": {
    "devreplay": "./bin/devreplay"
  },
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "create": "npm run build && npm run test",
    "compile": "tsc -p src",
    "publish:local": "sh ./scripts/npmPublish.sh",
    "test": "node ./bin/devreplay test/files/hello.ts test/rules/devreplay.json",
    "test:py": "node ./bin/devreplay test/files/loop.py test/rules/devreplay.json",
    "test:fix": "node ./bin/devreplay --fix test/files/hello.ts test/rules/devreplay.json",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "verify": "npm-run-all clean compile test"
  },
  "dependencies": {
    "@babel/code-frame": "^7.0.0",
    "builtin-modules": "^3.1.0",
    "commander": "^2.20.0",
    "tslib": "^1.10.0",
    "tsutils": "^3.17.0",
    "unicode": "^11.0.1"
  },
  "devDependencies": {
    "@types/node": "^12.0.8",
    "@types/rimraf": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ikuyadeu/devreplay.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/ikuyadeu",
    "name": "@ikuyadeu/devreplay"
  },
  "keywords": [
    "cli",
    "linter"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ikuyadeu/devreplay/issues"
  },
  "homepage": "https://github.com/ikuyadeu/devreplay#readme",
  "author": "Yuki Ueda"
}
