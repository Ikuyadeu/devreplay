{
  "name": "devreplay",
  "version": "0.0.4",
  "displayName": "devreplay",
  "description": "Replay your source code development",
  "bin": {
    "devreplay": "./bin/devreplay"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.helloWorld"
  ],
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "create": "npm run build && npm run test",
    "compile": "tsc -p src",
    "test": "node ./bin/dev-replay src/devreplayCli.ts",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "verify": "npm-run-all clean compile test"
  },
  "dependencies": {
    "@babel/code-frame": "^7.0.0",
    "builtin-modules": "^1.1.1",
    "vscode-textmate": "^4.1.1",
    "tslib": "^1.10.0",
    "tsutils": "^3.14.0"
  },
  "devDependencies": {
    "@types/node": "^12.0.8",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2",
    "npm-run-all":"^4.1.5"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikuyadeu/devreplay.git"
  },
  "keywords": [
    "cli",
    "linter"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ikuyadeu/devreplay/issues"
  },
  "homepage": "https://github.com/ikuyadeu/devreplay#readme",
  "author": "Yuki Ueda"
}
